{
  "name": "RaketaStars",
  "version": "1.0.0",
  "description": "🚀 Мини-приложение для Telegram на космическую тематику",
  "author": "RaketaStars Team",
  "keywords": ["telegram", "mini-app", "game", "space", "stars", "rocket"],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/raketastars.git"
  },
  "main": "index.html",
  "scripts": {
    "start": "python -m http.server 8000",
    "deploy": "echo 'Deploy to your hosting service'"
  },
  "telegram": {
    "short_name": "raketastars",
    "name": "RaketaStars",
    "description": "Собирайте звезды и запускайте ракеты в космосе! 🚀⭐",
    "icon": "🚀",
    "categories": ["games", "entertainment"],
    "features": [
      "Сбор звезд",
      "Запуск ракет", 
      "Система достижений",
      "Статистика игрока",
      "Красивые анимации"
    ]
  },
  "manifest": {
    "name": "RaketaStars",
    "short_name": "RaketaStars",
    "description": "Космическая игра для Telegram",
    "start_url": "/",
    "display": "standalone",
    "background_color": "#667eea",
    "theme_color": "#764ba2",
    "icons": [
      {
        "src": "data:image/svg+xml;base64,",
        "sizes": "64x64",
        "type": "image/svg+xml"
      }
    ]
  },
  "browser_support": {
    "chrome": ">=60",
    "firefox": ">=55",
    "safari": ">=12",
    "edge": ">=79"
  },
  "hosting": {
    "github_pages": {
      "enabled": true,
      "build_command": "echo 'No build needed'"
    },
    "netlify": {
      "enabled": true,
      "publish": "./",
      "build": {
        "command": "echo 'No build needed'",
        "publish": "./"
      }
    },
    "vercel": {
      "enabled": true,
      "builds": [
        {
          "src": "index.html",
          "use": "@vercel/static"
        }
      ]
    }
  }
} 